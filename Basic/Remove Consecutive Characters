

class Solution{
    public String removeConsecutiveCharacter(String s){
        
        String ans= "";
        ans += s.charAt(0);
        for(int i=1;i<s.length();i++){
            if(s.charAt(i-1)!=s.charAt(i)){
               ans += s.charAt(i);
            }
        }
        return ans;
    }
}

